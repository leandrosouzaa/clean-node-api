{
   "extends": "standard-with-typescript",
   "parserOptions": {
      "project": "./tsconfig.json"
   },

   "rules": {
      "@typescript-eslint/strict-boolean-expressions": "off",
      "@typescript-eslint/consistent-type-definition": "off",
